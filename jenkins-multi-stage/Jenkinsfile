pipeline{
     agent { label 'MyNode' }
stages {
        stage ('Clone') {
            steps {
                git branch: 'main', url: "https://github.com/hossain109/CICDMavenProject.git"
            }
        }
     
        stage("Build and start test image") {
            steps {
                sh "docker-composer build"
                sh "docker-compose up -d"
            }
        }
      }
}